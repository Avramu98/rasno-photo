{
  "extends": [
    "next/core-web-vitals",
    "airbnb-typescript",
    "plugin:import/recommended"
  ],
  "plugins": [
    "import"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "import/first": "error",
    "import/extensions": "off",
    "import/newline-after-import": "error",
    "import/no-duplicates": "error",
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index"
        ],
        "newlines-between": "always"
      }
    ]
  }
}
